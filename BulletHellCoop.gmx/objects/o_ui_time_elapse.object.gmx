// Save the start time
global.start_time = date_current_time();
/*
   Calculate the elapsed time
*/
time_used = date_current_time()-global.start_time;
/*
   First we extract the hours, minutes
   and seconds as real numbers
*/
hours = date_get_hour(time_used);
minutes = date_get_minute(time_used);
seconds = date_get_second(time_used);
/*
   Then we convert them to strings,
   adding a leading zero if required
*/
hrs = string(hours);
if(hours &lt; 10) {hrs = "0"+hrs;}
mins = string(minutes);
if(minutes &lt; 10) {mins = "0"+mins;}
secs = string(seconds);
if(seconds &lt; 10) {secs = "0"+secs;}

draw_text(x,y,hrs + ":" + mins + ":" + secs);
</string>
